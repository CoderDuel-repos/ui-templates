  "code": "const TeaChatHub = () => {\n  const [location, setLocation] = useState('');\n  const [selectedTime, setSelectedTime] = useState('afternoon');\n  const [showMatches, setShowMatches] = useState(false);\n\n  const nearbyUsers = [\n    { id: 1, name: 'Sarah Chen', distance: '0.8 mi', bio: 'Earl Grey enthusiast, love deep conversations', availability: 'Today 3PM', imageId: 64 },\n    { id: 2, name: 'Marcus Johnson', distance: '1.2 mi', bio: 'Green tea lover, into philosophy & tech', availability: 'Tomorrow 2PM', imageId: 91 },\n    { id: 3, name: 'Priya Patel', distance: '1.5 mi', bio: 'Chai aficionado, entrepreneur mindset', availability: 'Today 5PM', imageId: 177 },\n    { id: 4, name: 'Alex Rivera', distance: '2.1 mi', bio: 'Matcha devotee, creative writer', availability: 'Tomorrow 4PM', imageId: 338 }\n  ];\n\n  const upcomingMeetups = [\n    { cafe: 'The Steep Spot', time: 'Today 4:30 PM', attendees: 3, imageId: 30 },\n    { cafe: 'Zen Tea Lounge', time: 'Tomorrow 2:00 PM', attendees: 5, imageId: 431 }\n  ];\n\n  return (\n    <div className=\"min-h-screen bg-gradient-to-br from-slate-950 via-purple-950 to-slate-900 text-white relative overflow-hidden\">\n      {/* Animated background elements */}\n      <div className=\"absolute inset-0 opacity-20\">\n        <div className=\"absolute top-20 left-20 w-96 h-96 bg-purple-500 rounded-full blur-3xl animate-pulse\"></div>\n        <div className=\"absolute bottom-20 right-20 w-96 h-96 bg-cyan-500 rounded-full blur-3xl animate-pulse delay-1000\"></div>\n      </div>\n\n      {/* Main Content */}\n      <div className=\"relative z-10 max-w-7xl mx-auto px-4 py-8\">\n        {/* Header */}\n        <div className=\"flex items-center justify-between mb-12\">\n          <div className=\"flex items-center gap-3\">\n            <div className=\"w-12 h-12 bg-gradient-to-br from-purple-500 to-cyan-500 rounded-xl flex items-center justify-center text-2xl\">\n              üçµ\n            </div>\n            <span className=\"text-2xl font-bold bg-gradient-to-r from-purple-400 to-cyan-400 bg-clip-text text-transparent\">\n              TeaConnect\n            </span>\n          </div>\n          <button className=\"px-6 py-2 rounded-full bg-white/10 backdrop-blur-lg border border-white/20 hover:bg-white/20 transition-all\">\n            My Profile\n          </button>\n        </div>\n\n        {/* Hero Section */}\n        <div className=\"mb-16\">\n          <h1 className=\"text-6xl font-bold mb-6 bg-gradient-to-r from-white via-purple-200 to-cyan-200 bg-clip-text text-transparent\">{{HERO_TITLE}}</h1>\n          <p className=\"text-xl text-gray-300 mb-8 max-w-2xl\">{{HERO_SUBTITLE}}</p>\n\n          {/* Search Card */}\n          <div className=\"bg-white/10 backdrop-blur-xl rounded-3xl border border-white/20 p-8 shadow-2xl mb-8\">\n            <div className=\"grid md:grid-cols-3 gap-6\">\n              <div>\n                <label className=\"block text-sm font-medium mb-2 text-gray-300\">Your Location</label>\n                <input\n                  type=\"text\"\n                  value={location}\n                  onChange={(e) => setLocation(e.target.value)}\n                  placeholder=\"Enter city or zip code\"\n                  className=\"w-full px-4 py-3 rounded-xl bg-white/5 border border-white/10 focus:border-purple-500 focus:outline-none focus:ring-2 focus:ring-purple-500/50 transition-all\"\n                />\n              </div>\n              <div>\n                <label className=\"block text-sm font-medium mb-2 text-gray-300\">Preferred Time</label>\n                <select\n                  value={selectedTime}\n                  onChange={(e) => setSelectedTime(e.target.value)}\n                  className=\"w-full px-4 py-3 rounded-xl bg-white/5 border border-white/10 focus:border-cyan-500 focus:outline-none focus:ring-2 focus:ring-cyan-500/50 transition-all\"\n                >\n                  <option value=\"morning\">Morning</option>\n                  <option value=\"afternoon\">Afternoon</option>\n                  <option value=\"evening\">Evening</option>\n                  <option value=\"weekend\">Weekend</option>\n                </select>\n              </div>\n              <div className=\"flex items-end\">\n                <button\n                  onClick={() => setShowMatches(true)}\n                  className=\"w-full px-6 py-3 rounded-xl bg-gradient-to-r from-purple-600 to-cyan-600 hover:from-purple-500 hover:to-cyan-500 font-semibold shadow-lg shadow-purple-500/50 transition-all transform hover:scale-105\"\n                >\n                  Find Tea Buddies\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n\n        {/* Nearby Users Grid */}\n        <div className=\"mb-16\">\n          <div className=\"flex items-center justify-between mb-6\">\n            <h2 className=\"text-3xl font-bold\">{{FEATURE_TITLE}}</h2>\n            <span className=\"text-sm text-gray-400\">Updated 2 min ago</span>\n          </div>\n          \n          <div className=\"grid md:grid-cols-2 lg:grid-cols-4 gap-6\">\n            {nearbyUsers.map((user) => (\n              <div\n                key={user.id}\n                className=\"bg-white/5 backdrop-blur-lg rounded-2xl border border-white/10 overflow-hidden hover:border-purple-500/50 transition-all transform hover:scale-105 cursor-pointer group\"\n              >\n                <div className=\"relative h-48 overflow-hidden\">\n                  <img\n                    src={`https://picsum.photos/id/${user.imageId}/400/300`}\n                    alt={user.name}\n                    className=\"w-full h-full object-cover group-hover:scale-110 transition-transform duration-300\"\n                  />\n                  <div className=\"absolute top-3 right-3 px-3 py-1 rounded-full bg-black/50 backdrop-blur-sm text-xs font-medium\">\n                    {user.distance}\n                  </div>\n                </div>\n                <div className=\"p-5\">\n                  <h3 className=\"text-lg font-semibold mb-2\">{{FEATURE_1_TITLE}}</h3>\n                  <p className=\"text-sm text-gray-400 mb-3 line-clamp-2\">{user.bio}</p>\n                  <div className=\"flex items-center justify-between\">\n                    <span className=\"text-xs text-cyan-400 font-medium\">{user.availability}</span>\n                    <button className=\"px-4 py-1.5 rounded-full bg-gradient-to-r from-purple-600/80 to-cyan-600/80 text-xs font-medium hover:from-purple-600 hover:to-cyan-600 transition-all\">{{CTA_PRIMARY}}</button>\n                  </div>\n                </div>\n              </div>\n            ))}\n          </div>\n        </div>\n\n        {/* Upcoming Meetups */}\n        <div className=\"mb-16\">\n          <h2 className=\"text-3xl font-bold mb-6\">{{FEATURE_2_TITLE}}</h2>\n          <div className=\"grid md:grid-cols-2 gap-6\">\n            {upcomingMeetups.map((meetup, index) => (\n              <div\n                key={index}\n                className=\"bg-gradient-to-br from-white/10 to-white/5 backdrop-blur-xl rounded-2xl border border-white/20 overflow-hidden hover:border-cyan-500/50 transition-all\"\n              >\n                <div className=\"flex\">\n                  <div className=\"w-1/3\">\n                    <img\n                      src={`https://picsum.photos/id/${meetup.imageId}/300/300`}\n                      alt={meetup.cafe}\n                      className=\"w-full h-full object-cover\"\n                    />\n                  </div>\n                  <div className=\"w-2/3 p-6\">\n                    <h3 className=\"text-xl font-semibold mb-2\">{{FEATURE_3_TITLE}}</h3>\n                    <p className=\"text-gray-300 mb-4\">{meetup.time}</p>\n                    <div className=\"flex items-center justify-between\">\n                      <div className=\"flex items-center gap-2\">\n                        <div className=\"flex -space-x-2\">\n                          {[1, 2, 3].map((i) => (\n                            <div key={i} className=\"w-8 h-8 rounded-full bg-gradient-to-br from-purple-500 to-cyan-500 border-2 border-slate-900\"></div>\n                          ))}\n                        </div>\n                        <span className=\"text-sm text-gray-400\">+{meetup.attendees} attending</span>\n                      </div>\n                      <button className=\"px-5 py-2 rounded-full bg-white/10 hover:bg-white/20 text-sm font-medium transition-all\">\n                        Join\n                      </button>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            ))}\n          </div>\n        </div>\n\n        {/* CTA Section */}\n        <div className=\"bg-gradient-to-r from-purple-600/20 to-cyan-600/20 backdrop-blur-xl rounded-3xl border border-white/20 p-12 text-center\">\n          <h2 className=\"text-4xl font-bold mb-4\">{{FEATURE_4_TITLE}}</h2>\n          <p className=\"text-xl text-gray-300 mb-8 max-w-2xl mx-auto\">\n            Join thousands of tea enthusiasts making meaningful connections in their local communities\n          </p>\n          <button className=\"px-8 py-4 rounded-full bg-gradient-to-r from-purple-600 to-cyan-600 hover:from-purple-500 hover:to-cyan-500 font-semibold text-lg shadow-2xl shadow-purple-500/50 transition-all transform hover:scale-105\">{{CTA_SECONDARY}}</button>\n        </div>\n      </div>\n    </div>\n  );\n};",
