e) => setSelectedCity(e.target.value)}\n                    className=\"flex-1 bg-transparent text-white outline-none\"\n                  >\n                    <option value=\"San Francisco\">San Francisco, CA</option>\n                    <option value=\"New York\">New York, NY</option>\n                    <option value=\"Austin\">Austin, TX</option>\n                    <option value=\"Seattle\">Seattle, WA</option>\n                  </select>\n                </div>\n                <div className=\"flex-1 flex items-center gap-3 px-4 py-3 bg-white/5 rounded-xl\">\n                  <span className=\"text-2xl\">üîç</span>\n                  <input \n                    type=\"text\"\n                    placeholder=\"Interests, skills, or topics...\"\n                    className=\"flex-1 bg-transparent text-white placeholder-gray-500 outline-none\"\n                  />\n                </div>\n                <button \n                  onClick={() => setIsSearching(!isSearching)}\n                  className=\"px-8 py-3 bg-gradient-to-r from-purple-600 to-cyan-600 rounded-xl text-white font-medium hover:shadow-lg hover:shadow-purple-500/50 transition-all\"\n                >\n                  Find Matches\n                </button>\n              </div>\n            </div>\n          </div>\n\n          {/* Stats */}\n          <div className=\"grid grid-cols-3 gap-6 max-w-3xl mx-auto mb-20\">\n            <div className=\"backdrop-blur-xl bg-white/5 border border-white/10 rounded-2xl p-6\">\n              <div className=\"text-3xl font-bold bg-gradient-to-r from-purple-400 to-cyan-400 bg-clip-text text-transparent mb-2\">\n                50K+\n              </div>\n              <div className=\"text-gray-400 text-sm\">Active Members</div>\n            </div>\n            <div className=\"backdrop-blur-xl bg-white/5 border border-white/10 rounded-2xl p-6\">\n              <div className=\"text-3xl font-bold bg-gradient-to-r from-purple-400 to-cyan-400