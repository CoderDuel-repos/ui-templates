lassName={`px-4 py-2 rounded-xl font-medium transition-all ${\n                  activeTab === 'nearby'\n                    ? 'bg-white/20 text-white'\n                    : 'text-white/60 hover:text-white'\n                }`}\n              >\n                Nearby\n              </button>\n              <button\n                onClick={() => setActiveTab('recommended')}\n                className={`px-4 py-2 rounded-xl font-medium transition-all ${\n                  activeTab === 'recommended'\n                    ? 'bg-white/20 text-white'\n                    : 'text-white/60 hover:text-white'\n                }`}\n              >\n                Recommended\n              </button>\n            </div>\n          </div>\n\n          <div className=\"grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-6\">\n            {nearbyUsers.map((user) => (\n              <div\n                key={user.id}\n                className=\"backdrop-blur-xl bg-white/5 rounded-2xl p-6 border border-white/10 hover:border-purple-400/50 transition-all hover:shadow-xl hover:shadow-purple-500/20 group cursor-pointer\"\n              >\n                <div className=\"relative mb-4\">\n                  <img\n                    src={user.image}\n                    alt={user.name}\n                    className=\"w-full h-48 object-cover rounded-xl\"\n                  />\n                  <div className=\"absolute top-3 right-3 backdrop-blur-xl bg-white/20 px-3 py-1 rounded-full text-white text-sm font-medium border border-white/30\">\n                    {user.distance}\n                  </div>\n                </div>\n                <h3 className=\"text-white font-semibold text-lg mb-2\">{{FEATURE_2_TITLE}}</h3>\n                <p className=\"text-white/60 text-sm mb-3\">{user.interests}</p>\n                <div className=\"flex items-center gap-2 text-white/50 text-sm mb-4\">\n                  <span>üïê</span>\n                  <span>{user.availability}</span>\n    