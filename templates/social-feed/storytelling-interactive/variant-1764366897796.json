div className=\"max-w-7xl mx-auto grid md:grid-cols-2 gap-12 items-center\">\n          <div \n            className=\"space-y-6\"\n            style={{\n              opacity: activeStory >= 1 ? 1 : 0.3,\n              transform: activeStory >= 1 ? 'translateX(0)' : 'translateX(-50px)',\n              transition: 'all 0.8s ease-out'\n            }}\n          >\n            <div className=\"text-amber-600 font-semibold text-lg\">Step 01</div>\n            <h2 className=\"text-5xl font-bold text-gray-900 leading-tight\">{{FEATURE_TITLE}}</h2>\n            <h3 className=\"text-3xl text-amber-700 font-light\">{{FEATURE_1_TITLE}}</h3>\n            <p className=\"text-xl text-gray-700 leading-relaxed\">{{DESCRIPTION_1}}</p>\n            <div className=\"flex gap-4 pt-4\">\n              <div className=\"bg-white p-4 rounded-lg shadow-lg\">\n                <div className=\"text-3xl font-bold text-amber-600\">2.5K+</div>\n                <div className=\"text-sm text-gray-600\">Active Members</div>\n              </div>\n              <div className=\"bg-white p-4 rounded-lg shadow-lg\">\n                <div className=\"text-3xl font-bold text-amber-600\">150+</div>\n                <div className=\"text-sm text-gray-600\">Coffee Spots</div>\n              </div>\n            </div>\n          </div>\n          <div \n            className=\"relative\"\n            style={{\n              opacity: activeStory >= 1 ? 1 : 0.3,\n              transform: activeStory >= 1 ? 'translateX(0) scale(1)' : 'translateX(50px) scale(0.9)',\n              transition: 'all 0.8s ease-out'\n            }}\n          >\n            <img \n              src={stories[0].image}\n              alt=\"Coffee connection\"\n              className=\"w-full rounded-2xl shadow-2xl\"\n            />\n            <div className=\"absolute -bottom-6 -right-6 bg-white p-6 rounded-xl shadow-xl\">\n              <div className=\"text-sm text-gray-600 mb-1\">Next Coffee Chat</div>\n              <div clas