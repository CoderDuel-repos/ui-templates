  "code": "const CoffeeChatHero = () => {\n  const [scrollY, setScrollY] = useState(0);\n  const [activeStory, setActiveStory] = useState(0);\n  const [showMatchAnimation, setShowMatchAnimation] = useState(false);\n\n  useEffect(() => {\n    const handleScroll = () => {\n      setScrollY(window.scrollY);\n      \n      const scrollPosition = window.scrollY;\n      if (scrollPosition < 400) setActiveStory(0);\n      else if (scrollPosition < 1000) setActiveStory(1);\n      else if (scrollPosition < 1600) setActiveStory(2);\n      else setActiveStory(3);\n    };\n\n    window.addEventListener('scroll', handleScroll);\n    return () => window.removeEventListener('scroll', handleScroll);\n  }, []);\n\n  const stories = [\n    {\n      title: \"Meet Someone New\",\n      subtitle: \"Over Your Favorite Brew\",\n      description: \"Connect with like-minded people in your neighborhood who share your interests\",\n      image: \"https://picsum.photos/id/30/800/600\"\n    },\n    {\n      title: \"Share Your Story\",\n      subtitle: \"One Conversation at a Time\",\n      description: \"Every coffee chat is a chance to learn something new and make meaningful connections\",\n      image: \"https://picsum.photos/id/431/800/600\"\n    },\n    {\n      title: \"Find Your Community\",\n      subtitle: \"In Local Coffee Spots\",\n      description: \"Discover hidden gems and build relationships in your favorite cafes\",\n      image: \"https://picsum.photos/id/225/800/600\"\n    }\n  ];\n\n  const matches = [\n    { name: \"Sarah Chen\", interest: \"Startup Founder\", distance: \"0.8 miles\", image: \"https://picsum.photos/id/64/200/200\" },\n    { name: \"Marcus Johnson\", interest: \"Photography\", distance: \"1.2 miles\", image: \"https://picsum.photos/id/91/200/200\" },\n    { name: \"Emma Rodriguez\", interest: \"Book Club\", distance: \"0.5 miles\", image: \"https://picsum.photos/id/177/200/200\" }\n  ];\n\n  return (\n    <div className=\"min-h-screen bg-gradient-to-b from-