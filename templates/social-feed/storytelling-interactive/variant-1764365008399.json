  "code": "const CoffeeChatLanding = () => {\n  const [scrollY, setScrollY] = useState(0);\n  const [activeStory, setActiveStory] = useState(0);\n\n  useEffect(() => {\n    const handleScroll = () => setScrollY(window.scrollY);\n    window.addEventListener('scroll', handleScroll);\n    return () => window.removeEventListener('scroll', handleScroll);\n  }, []);\n\n  const stories = [\n    {\n      name: \"Sarah & Michael\",\n      location: \"Downtown Seattle\",\n      story: \"Met over coffee, now business partners\",\n      image: \"https://picsum.photos/id/64/400/400\"\n    },\n    {\n      name: \"James & Lisa\",\n      location: \"Brooklyn, NY\",\n      story: \"Coffee chat turned into lifelong friendship\",\n      image: \"https://picsum.photos/id/65/400/400\"\n    },\n    {\n      name: \"Emma & David\",\n      location: \"Austin, TX\",\n      story: \"Found their co-founder over a latte\",\n      image: \"https://picsum.photos/id/91/400/400\"\n    }\n  ];\n\n  const heroOpacity = Math.max(0, 1 - scrollY / 500);\n  const storyScale = Math.min(1, scrollY / 300);\n\n  return (\n    <div className=\"min-h-screen bg-gradient-to-b from-amber-50 via-orange-50 to-amber-100\">\n      {/* Hero Section with Parallax */}\n      <div className=\"relative min-h-screen flex items-center justify-center overflow-hidden\">\n        <div \n          className=\"absolute inset-0 z-0\"\n          style={{ transform: `translateY(${scrollY * 0.5}px)` }}\n        >\n          <img \n            src=\"https://picsum.photos/id/30/1920/1080\" \n            alt=\"Coffee shop atmosphere\"\n            className=\"w-full h-full object-cover opacity-40\"\n          />\n        </div>\n        \n        <div \n          className=\"relative z-10 max-w-4xl mx-auto px-6 text-center\"\n          style={{ opacity: heroOpacity }}\n        >\n          <h1 className=\"text-6xl md:text-7xl font-bold text-gray-900 mb-6\">{{HERO_TITLE}}</h1>\n          <p className=\"text-xl md:text-2xl text-gray-700 mb-12 max-w-2xl mx-auto\">{{DESCRIPTION}}</p>\n          <button className=\"bg-amber-600 text-white px-10 py-4 rounded-full text-lg font-semibold hover:bg-amber-700 transform hover:scale-105 transition-all shadow-xl\">{{CTA_PRIMARY}}</button>\n        </div>\n\n        <div className=\"absolute bottom-8 left-1/2 transform -translate-x-1/2 animate-bounce\">\n          <div className=\"w-6 h-10 border-2 border-amber-600 rounded-full flex items-start justify-center p-2\">\n            <div className=\"w-1 h-3 bg-amber-600 rounded-full\"></div>\n          </div>\n        </div>\n      </div>\n\n      {/* Story Timeline Section */}\n      <div className=\"py-20 px-6\">\n        <div \n          className=\"max-w-6xl mx-auto\"\n          style={{ transform: `scale(${storyScale})`, opacity: storyScale }}\n        >\n          <h2 className=\"text-5xl font-bold text-center text-gray-900 mb-4\">{{FEATURE_TITLE}}</h2>\n          <p className=\"text-xl text-center text-gray-600 mb-16\">{{DESCRIPTION_1}}</p>\n\n          <div className=\"grid md:grid-cols-3 gap-8 mb-20\">\n            {stories.map((story, index) => (\n              <div\n                key={index}\n                className=\"bg-white rounded-2xl overflow-hidden shadow-lg hover:shadow-2xl transition-all transform hover:-translate-y-2 cursor-pointer\"\n                onClick={() => setActiveStory(index)}\n              >\n                <img \n                  src={story.image} \n                  alt={story.name}\n                  className=\"w-full h-64 object-cover\"\n                />\n                <div className=\"p-6\">\n                  <h3 className=\"text-2xl font-bold text-gray-900 mb-2\">{{FEATURE_1_TITLE}}</h3>\n                  <p className=\"text-amber-600 font-semibold mb-3\">{{DESCRIPTION_2}}</p>\n                </div>\n              </div>\n            ))}\n          </div>\n        </div>\n      </div>\n\n      {/* Interactive How It Works */}\n      <div className=\"py-20 bg-white\">\n        <div className=\"max-w-6xl mx-auto px-6\">\n          <h2 className=\"text-5xl font-bold text-center text-gray-900 mb-16\">{{FEATURE_2_TITLE}}</h2>\n\n          <div className=\"grid md:grid-cols-3 gap-12\">\n            <div className=\"text-center space-y-6\">\n              <div className=\"w-20 h-20 bg-amber-100 rounded-full flex items-center justify-center mx-auto\">\n                <span className=\"text-3xl font-bold text-amber-600\">1</span>\n              </div>\n              <img \n                src=\"https://picsum.photos/id/431/600/400\" \n                alt=\"Create profile\"\n                className=\"w-full h-48 object-cover rounded-xl shadow-lg\"\n              />\n              <h3 className=\"text-2xl font-bold text-gray-900\">{{FEATURE_3_TITLE}}</h3>\n              <p className=\"text-gray-600\">{{DESCRIPTION_3}}</p>\n            </div>\n\n            <div className=\"text-center space-y-6\">\n              <div className=\"w-20 h-20 bg-amber-100 rounded-full flex items-center justify-center mx-auto\">\n                <span className=\"text-3xl font-bold text-amber-600\">2</span>\n              </div>\n              <img \n                src=\"https://picsum.photos/id/177/600/400\" \n                alt=\"Find matches\"\n                className=\"w-full h-48 object-cover rounded-xl shadow-lg\"\n              />\n              <h3 className=\"text-2xl font-bold text-gray-900\">{{FEATURE_4_TITLE}}</h3>\n              <p className=\"text-gray-600\">{{DESCRIPTION_4}}</p>\n            </div>\n\n            <div className=\"text-center space-y-6\">\n              <div className=\"w-20 h-20 bg-amber-100 rounded-full flex items-center justify-center mx-auto\">\n                <span className=\"text-3xl font-bold text-amber-600\">3</span>\n              </div>\n              <img \n                src=\"https://picsum.photos/id/225/600/400\" \n                alt=\"Meet for coffee\"\n                className=\"w-full h-48 object