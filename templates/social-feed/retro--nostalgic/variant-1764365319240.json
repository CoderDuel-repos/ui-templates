  "code": "const CoffeeChatHero = () => {\n  const [location, setLocation] = useState('');\n  const [isHovered, setIsHovered] = useState(false);\n\n  const featuredMembers = [\n    { name: 'Sarah Mitchell', interest: 'Startup Founder', image: 'https://picsum.photos/id/64/200/200' },\n    { name: 'James Park', interest: 'Coffee Enthusiast', image: 'https://picsum.photos/id/91/200/200' },\n    { name: 'Emma Rodriguez', interest: 'Designer', image: 'https://picsum.photos/id/177/200/200' },\n    { name: 'Michael Chen', interest: 'Writer', image: 'https://picsum.photos/id/338/200/200' }\n  ];\n\n  return (\n    <div className=\"min-h-screen bg-gradient-to-b from-amber-50 via-orange-50 to-amber-100\">\n      {/* Vintage Header */}\n      <header className=\"border-b-4 border-amber-900 bg-amber-100 shadow-lg\">\n        <div className=\"max-w-7xl mx-auto px-6 py-6\">\n          <div className=\"flex items-center justify-between\">\n            <div className=\"flex items-center gap-3\">\n              <div className=\"w-12 h-12 bg-amber-900 rounded-full flex items-center justify-center shadow-md\">\n                <span className=\"text-2xl\">☕</span>\n              </div>\n              <h1 className=\"text-3xl font-bold text-amber-900\" style={{ fontFamily: 'Georgia, serif' }}>{{HERO_TITLE}}</h1>\n            </div>\n            <nav className=\"hidden md:flex gap-8\">\n              <a href=\"#\" className=\"text-amber-900 hover:text-amber-700 font-semibold transition-colors\">Home</a>\n              <a href=\"#\" className=\"text-amber-900 hover:text-amber-700 font-semibold transition-colors\">Find Chats</a>\n              <a href=\"#\" className=\"text-amber-900 hover:text-amber-700 font-semibold transition-colors\">Sign In</a>\n            </nav>\n          </div>\n        </div>\n      </header>\n\n      {/* Hero Section */}\n      <div className=\"max-w-7xl mx-auto px-6 py-16\">\n        <div className=\"grid md:grid-cols-2 gap-12 items-center mb-16\">\n          <div className=\"space-y-8\">\n            <div className=\"inline-block bg-amber-900 text-amber-50 px-6 py-2 rounded-full text-sm font-semibold shadow-md transform -rotate-1\">\n              ★ Est. 2024 ★\n            </div>\n            \n            <h2 className=\"text-6xl font-bold text-amber-900 leading-tight\" style={{ fontFamily: 'Georgia, serif' }}>{{FEATURE_TITLE}}</h2>\n            \n            <p className=\"text-xl text-amber-800 leading-relaxed\" style={{ fontFamily: 'Georgia, serif' }}>\n              Connect with interesting folks in your neighborhood over a warm cup of joe. \n              Because the best conversations happen face-to-face.\n            </p>\n\n            <div className=\"bg-white border-4 border-amber-900 rounded-lg shadow-xl p-6\">\n              <label className=\"block text-amber-900 font-bold mb-3 text-lg\" style={{ fontFamily: 'Georgia, serif' }}>\n                Where are you located?\n              </label>\n              <div className=\"flex gap-3\">\n                <input\n                  type=\"text\"\n                  value={location}\n                  onChange={(e) => setLocation(e.target.value)}\n                  placeholder=\"Enter your city or zip code\"\n                  className=\"flex-1 px-4 py-3 border-3 border-amber-900 rounded-lg focus:outline-none focus:ring-4 focus:ring-orange-300 text-lg\"\n                  style={{ fontFamily: 'Georgia, serif' }}\n                />\n                <button \n                  className=\"bg-amber-900 text-amber-50 px-8 py-3 rounded-lg font-bold text-lg hover:bg-amber-800 transition-all transform hover:scale-105 shadow-lg\"\n                  onMouseEnter={() => setIsHovered(true)}\n                  onMouseLeave={() => setIsHovered(false)}\n                  style={{ fontFamily: 'Georgia, serif' }}\n                >\n                  {isHovered ? '☕ Find' : 'Find'}\n                </button>\n              </div>\n            </div>\n\n            <div className=\"flex gap-4 text-amber-800\">\n              <div className=\"flex items-center gap-2\">\n                <span className=\"text-2xl\">✓</span>\n                <span className=\"font-semibold\">Free to Join</span>\n              </div>\n              <div className=\"flex items-center gap-2\">\n                <span className=\"text-2xl\">✓</span>\n                <span className=\"font-semibold\">Safe & Verified</span>\n              </div>\n            </div>\n          </div>\n\n          <div className=\"relative\">\n            <div className=\"absolute -top-4 -left-4 w-full h-full bg-amber-900 rounded-lg transform rotate-2\"></div>\n            <div className=\"relative bg-white border-4 border-amber-900 rounded-lg overflow-hidden shadow-2xl\">\n              <img \n                src=\"https://picsum.photos/id/30/800/600\" \n                alt=\"Coffee shop meeting\"\n                className=\"w-full h-96 object-cover\"\n              />\n              <div className=\"absolute bottom-0 left-0 right-0 bg-gradient-to-t from-amber-900 to-transparent p-6\">\n                <p className=\"text-amber-50 text-xl font-bold\" style={{ fontFamily: 'Georgia, serif' }}>\n                  \"Met amazing people in my area!\"\n                </p>\n              </div>\n            </div>\n          </div>\n        </div>\n\n        {/* Featured Members Section */}\n        <div className=\"bg-orange-100 border-4 border-amber-900 rounded-lg p-8 shadow-xl\">\n          <div className=\"text-center mb-8\">\n            <h3 className=\"text-3xl font-bold text-amber-900 mb-2\" style={{ fontFamily: 'Georgia, serif' }}>{{FEATURE_1_TITLE}}</h3>\n            <p className=\"text-amber-800 text-lg\" style={{ fontFamily: 'Georgia, serif' }}>\n              Join thousands of friendly folks ready to chat\n            </p>\n          </div>\n\n          <div className=\"grid grid-cols-2 md:grid-cols-4 gap-6\">\n            {featuredMembers.map((member, index) => (\n              <div \n                key={index}\n                className=\"bg-white border-3 border-amber-900 rounded-lg p-4 shadow-lg hover:shadow-2xl transition-all transform hover:-translate-y-2\"\n              >\n                <div className=\"relative mb-4\">\n                  <img \n                    src={member.image}\n                    alt={member.name}\n                    className=\"w-full h-48 object-cover rounded-lg border-2 border-amber-900\"\n                  />\n                  <div className=\"absolute -bottom-3 -right-3 bg-amber-900 text-amber-50 w-12 h-12 rounded-full flex items-center justify-center text-2xl shadow-lg\">\n                    ☕\n                  </div>\n                </div>\n                <h4 className=\"font-bold text-amber-900 text-lg mb-1\" style={{ fontFamily: 'Georgia, serif' }}>\n                  {member.name}\n                </h4>\n                <p className=\"text-amber-700 text-sm\" style={{ fontFamily: 'Georgia, serif' }}>\n                  {member.interest}\n                </p>\n              </div>\n            ))}\n          </div>\n        </div>\n\n        {/* How It Works */}\n        <div className=\"mt-16 grid md:grid-cols-3 gap-8\">\n          {[\n            { step: '1', title: 'Create Profile', desc: 'Tell us about yourself and your coffee preferences' },\n            { step: '2', title: 'Find Matches', desc: 'Browse local coffee enthusiasts in your area' },\n            { step: '3', title: 'Grab Coffee', desc: 'Schedule a meetup at your favorite local spot' }\n          ].map((item, index) => (\n            <div key={index} className=\"bg-white border-4 border-amber-900 rounded-lg p-6 shadow-lg text-center transform hover:rotate-1 transition-transform\">\n              <div className=\"w-16 h-16 bg-amber-900 text-amber-50 rounded-full flex items-center justify-center text-3xl font-bold mx-auto mb-4 shadow-md\" style={{ fontFamily: 'Georgia, serif' }}>\n                {item.step}\n              </div>\n              <h4 className=\"text-xl font-bold text-amber-900 mb-2\" style={{ fontFamily: 'Georgia, serif' }}>\n                {item.title}\n              </h4>\n              <p className=\"text-amber-800\" style={{ fontFamily: 'Georgia, serif' }}>\n                {item.desc}\n              </p>\n            </div>\n          ))}\n        </div>\n      </div>\n    </div>\n  );\n};",
