  "code": "const BoardGameConnect = () => {\n  const [location, setLocation] = useState('');\n  const [selectedGame, setSelectedGame] = useState(null);\n\n  const featuredGames = [\n    { id: 1, name: 'Settlers of Catan', players: '3-4', time: '90 min', img: 'https://picsum.photos/id/244/400/300' },\n    { id: 2, name: 'Ticket to Ride', players: '2-5', time: '60 min', img: 'https://picsum.photos/id/274/400/300' },\n    { id: 3, name: 'Pandemic', players: '2-4', time: '45 min', img: 'https://picsum.photos/id/367/400/300' },\n  ];\n\n  const upcomingMeetups = [\n    { id: 1, title: 'Board Game Night at The Coffee House', location: 'Downtown', date: 'Friday, 7:00 PM', attendees: 8, img: 'https://picsum.photos/id/0/200/150' },\n    { id: 2, title: 'Strategy Games Meetup', location: 'Westside Community Center', date: 'Saturday, 2:00 PM', attendees: 12, img: 'https://picsum.photos/id/180/200/150' },\n  ];\n\n  return (\n    <div className=\"min-h-screen bg-amber-50\">\n      {/* Header */}\n      <header className=\"bg-gradient-to-r from-orange-700 to-red-700 text-amber-50 shadow-lg border-b-4 border-amber-900\">\n        <div className=\"max-w-7xl mx-auto px-4 py-6\">\n          <div className=\"flex items-center justify-between\">\n            <div className=\"flex items-center gap-3\">\n              <div className=\"w-12 h-12 bg-amber-100 rounded-lg flex items-center justify-center transform rotate-12 shadow-md\">\n                <span className=\"text-3xl\">üé≤</span>\n              </div>\n              <h1 className=\"text-3xl font-bold\" style={{ fontFamily: 'Georgia, serif' }}>{{HERO_TITLE}}</h1>\n            </div>\n            <nav className=\"hidden md:flex gap-6\">\n              <a href=\"#\" className=\"hover:text-amber-200 transition-colors font-semibold\">Find Games</a>\n              <a href=\"#\" className=\"hover:text-amber-200 transition-colors font-semibold\">Host Event</a>\n              <a href=\"#\" className=\"hover:text-amber-200 transition-colors font-semibold\">My Games</a>\n            </nav>\n          </div>\n        </div>\n      </header>\n\n      {/* Hero Section */}\n      <div className=\"bg-gradient-to-b from-orange-100 to-amber-50 border-b-4 border-orange-300\">\n        <div className=\"max-w-7xl mx-auto px-4 py-16\">\n          <div className=\"text-center mb-12\">\n            <h2 className=\"text-5xl font-bold text-orange-900 mb-4\" style={{ fontFamily: 'Georgia, serif' }}>{{FEATURE_TITLE}}</h2>\n            <p className=\"text-xl text-orange-800 mb-8\" style={{ fontFamily: 'Georgia, serif' }}>{{DESCRIPTION}}</p>\n            \n            {/* Search Bar */}\n            <div className=\"max-w-2xl mx-auto bg-white rounded-lg shadow-xl border-4 border-orange-900 p-6\">\n              <div className=\"flex flex-col md:flex-row gap-4\">\n                <input\n                  type=\"text\"\n                  placeholder=\"Enter your city or zip code...\"\n                  value={location}\n                  onChange={(e) => setLocation(e.target.value)}\n                  className=\"flex-1 px-6 py-4 border-2 border-orange-300 rounded-lg text-lg focus:outline-none focus:border-orange-500\"\n                  style={{ fontFamily: 'Georgia, serif' }}\n                />\n                <button className=\"bg-orange-700 hover:bg-orange-800 text-white px-8 py-4 rounded-lg font-bold text-lg shadow-lg transform hover:scale-105 transition-all border-2 border-orange-900\">{{CTA_PRIMARY}}</button>\n              </div>\n            </div>\n          </div>\n\n          {/* Stats Banner */}\n          <div className=\"grid grid-cols-3 gap-6 max-w-3xl mx-auto\">\n            <div className=\"bg-amber-100 border-4 border-amber-900 rounded-lg p-6 text-center shadow-lg\">\n              <div className=\"text-4xl font-bold text-orange-900 mb-2\">2,847</div>\n              <div className=\"text-orange-800 font-semibold\">Active Players</div>\n            </div>\n            <div className=\"bg-amber-100