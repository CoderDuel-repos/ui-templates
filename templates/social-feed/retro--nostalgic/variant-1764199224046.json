  "code": "const CoffeeChat = () => {\n  const [selectedCity, setSelectedCity] = useState('San Francisco');\n  const [showMatchModal, setShowMatchModal] = useState(false);\n\n  const cities = ['San Francisco', 'New York', 'Austin', 'Seattle', 'Portland'];\n  \n  const featuredMembers = [\n    { name: 'Sarah Chen', interests: 'Startups, Design, Hiking', photo: 'https://picsum.photos/id/64/400/400', distance: '0.8 miles' },\n    { name: 'Mike Rodriguez', interests: 'Photography, Jazz, Books', photo: 'https://picsum.photos/id/91/400/400', distance: '1.2 miles' },\n    { name: 'Emma Thompson', interests: 'Travel, Coffee, Writing', photo: 'https://picsum.photos/id/177/400/400', distance: '0.5 miles' },\n    { name: 'James Park', interests: 'Tech, Gaming, Cooking', photo: 'https://picsum.photos/id/338/400/400', distance: '1.8 miles' }\n  ];\n\n  return (\n    <div className=\"min-h-screen bg-gradient-to-b from-amber-50 via-orange-50 to-yellow-50\">\n      {/* Retro Header */}\n      <header className=\"bg-gradient-to-r from-orange-600 to-amber-600 shadow-lg border-b-4 border-amber-800\">\n        <div className=\"max-w-7xl mx-auto px-6 py-6\">\n          <div className=\"flex items-center justify-between\">\n            <div className=\"flex items-center gap-3\">\n              <div className=\"w-12 h-12 bg-amber-900 rounded-full flex items-center justify-center shadow-inner\">\n                <span className=\"text-2xl\">☕</span>\n              </div>\n              <div>\n                <h1 className=\"text-3xl font-bold text-amber-50 tracking-tight\" style={{ fontFamily: 'Georgia, serif' }}>\n                  Coffee Connections\n                </h1>\n                <p className=\"text-amber-100 text-sm italic\">Est. 2024 • Local Meetups</p>\n              </div>\n            </div>\n            <button className=\"bg-amber-900 text-amber-50 px-6 py-3 rounded-lg font-semibold shadow-lg hover:bg-amber-800 transition-all border-2 border-amber-950\">\n              Sign In\n            </button>\n          </div>\n        </div>\n      </header>\n\n      {/* Hero Section */}\n      <div className=\"max-w-7xl mx-auto px-6 py-12\">\n        <div className=\"grid md:grid-cols-2 gap-12 items-center mb-16\">\n          <div className=\"space-y-6\">\n            <div className=\"inline-block bg-amber-800 text-amber-50 px-4 py-2 rounded-full text-sm font-semibold shadow-md\">\n              ★ Join 10,000+ Coffee Lovers ★\n            </div>\n            <h2 className=\"text-6xl font-bold text-amber-900 leading-tight\" style={{ fontFamily: 'Georgia, serif' }}>\n              Meet Your Next\n              <span className=\"block text-orange-700\">Coffee Buddy</span>\n            </h2>\n            <p className=\"text-xl text-amber-800 leading-relaxed\">\n              Connect with friendly folks in your neighborhood. Share stories, ideas, and great conversation over a warm cup of joe.\n            </p>\n            \n            <div className=\"space-y-4\">\n              <div className=\"flex items-center gap-4\">\n                <label className=\"text-lg font-semibold text-amber-900\">Your City:</label>\n                <select \n                  value={selectedCity}\n                  onChange={(e) => setSelectedCity(e.target.value)}\n                  className=\"px-4 py-3 bg-white border-3 border-amber-800 rounded-lg text-amber-900 font-semibold shadow-md focus:outline-none focus:ring-4 focus:ring-orange-300\"\n                >\n                  {cities.map(city => (\n                    <option key={city} value={city}>{city}</option>\n                  ))}\n                </select>\n              </div>\n              \n              <button \n                onClick={() => setShowMatchModal(true)}\n                className=\"bg-gradient-to-r from-orange-600 to-amber-600 text-white px-8 py-4 rounded-lg font-bold text-lg shadow-xl hover:shadow-2xl hover:scale-105 transition-all border-3 border-amber-800\"\n              >\n                Find Coffee Buddies Near You →\n              </button>\n            </div>\n\n            <div className=\"flex gap-8 pt-4\">\n              <div className=\"text-center\">\n                <div className=\"text-3xl font-bold text-orange-700\">2,847</div>\n                <div className=\"text-sm text-amber-800\">Meetups This Week</div>\n              </div>\n              <div className=\"text-center\">\n                <div className=\"text-3xl font-bold text-orange-700\">156</div>\n                <div className=\"text-sm text-amber-800\">Local Cafes</div>\n              </div>\n              <div className=\"text-center\">\n                <div className=\"text-3xl font-bold text-orange-700\">4.9★</div>\n                <div className=\"text-sm text-amber-800\">Member Rating</div>\n              </div>\n            </div>\n          </div>\n\n          <div className=\"relative\">\n            <div className=\"absolute inset-0 bg-gradient-to-br from-orange-400 to-amber-500 rounded-3xl transform rotate-3 shadow-2xl\"></div>\n            <img \n              src=\"https://picsum.photos/id/30/800/600\" \n              alt=\"Coffee meetup\"\n              className=\"relative rounded-3xl shadow-2xl w-full h-96 object-cover border-8 border-amber-900\"\n            />\n            <div className=\"absolute -bottom-6 -right-6 bg-amber-900 text-amber-50 px-6 py-4 rounded-2xl shadow-xl border-4 border-amber-950\">\n              <div className=\"text-2xl font-bold\">Join Today!</div>\n              <div className=\"text-sm\">Free Forever</div>\n            </div>\n          </div>\n        </div>\n\n        {/* Featured Members Section */}\n        <div className=\"mb-16\">\n          <div className=\"text-center mb-10\">\n            <h3 className=\"text-4xl font-bold text-amber-900 mb-3\" style={{ fontFamily: 'Georgia, serif' }}>\n              Coffee Lovers Near You\n            </h3>\n            <p className=\"text-lg text-amber-800\">Real people, real connections, real coffee</p>\n          </div>\n\n          <div className=\"grid md:grid-cols-4 gap-6\">\n            {featuredMembers.map((member, idx) => (\n              <div \n                key={idx}\n                className=\"bg-white rounded-2xl shadow-lg border-4 border-amber-800 overflow-hidden hover:shadow-2xl hover:scale-105 transition-all\"\n              >\n                <div className=\"relative\">\n                  <img \n                    src={member.photo}\n                    alt={member.name}\n                    className=\"w-full h-64 object-cover\"\n                  />\n                  <div className=\"absolute top-3 right-3 bg-orange-600 text-white px-3 py-1 rounded-full text-sm font-semibold shadow-md\">\n                    {member.distance}\n                  </div>\n                </div>\n                <div className=\"p-5 space-y-3\">\n                  <h4 className=\"text-xl font-bold text-amber-900\">{member.name}</h4>\n                  <p className=\"text-sm text-amber-700 leading-relaxed\">{member.interests}</p>\n                  <button className=\"w-full bg-gradient-to-r from-orange-500 to-amber-500 text-white py-2 rounded-lg font-semibold hover:from-orange-600 hover:to-amber-600 transition-all shadow-md\">\n                    Say Hello ☕\n                  </button>\n                </div>\n              </div>\n            ))}\n          </div>\n        </div>\n\n        {/* How It Works */}\n        <div className=\"bg-gradient-to-r from-orange-600 to-amber-600 rounded-3xl p-12 shadow-2xl border-4 border-amber-900\">\n          <h3 className=\"text-4xl font-bold text-amber-50 text-center mb-12\" style={{ fontFamily: 'Georgia, serif' }}>\n            How It Works\n          </h3>\n          <div className=\"grid md:grid-cols-3 gap-8\">\n            <div className=\"text-center space-y-4\">\n              <div className=\"w-20 h-20 bg-amber-900 rounded-full flex items-center justify-center text-4xl mx-auto shadow-lg border-4 border-amber-950\">\n                1️⃣\n              </div>\n              <h4 className=\"text-2xl font-bold text-amber-50\">Create Profile</h4>\n              <p className=\"text-amber-100\">Share your interests and favorite coffee spots</p>\n            </div>\n            <div className=\"text-center space-y-4\">\n              <div className=\"w-20 h-20 bg-amber-900 rounded-full flex items-center justify-center text-4xl mx-auto shadow-lg border-4 border-amber-950\">\n                2️⃣\n              </div>\n              <h4 className=\"text-2xl font-bold text-amber-50\">Find Matches</h4>\n              <p className=\"text-amber-100\">Discover friendly people nearby who love coffee</p>\n            </div>\n            <div className=\"text-center space-y-4\">\n              <div className=\"w-20 h-20 bg-amber-900 rounded-full flex items-center justify-center text-4xl mx-auto shadow-lg border-4 border-amber-950\">\n                3️⃣\n              </div>\n              <h4 className=\"text-2xl font-bold text-amber-50\">Grab Coffee</h4>\n              <p className=\"text-amber-100\">Meet up at a local cafe and make new friends</p>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      {/* Match Modal */}\n      {showMatchModal && (\n        <div className=\"fixed inset-0 bg-black bg-opacity-60 flex items-center justify-center p-6 z-50\">\n          <div className=\"bg-gradient-to-b from-amber-50 to-orange-50 rounded-3xl p-8 max-w-md w-full shadow-2xl border-4 border-amber-900\">\n            <div className=\"text-center space-y-6\">\n              <div className=\"text-6xl\">☕</div>\n              <h3 className=\"text-3xl font-bold text-amber-900\" style={{ fontFamily: 'Georgia, serif' }}>\n                Welcome Aboard!\n              </h3>\n              <p className=\"text-lg text-amber-800\">\n                Create your free profile to start connecting with coffee lovers in {selectedCity}\n              </p>\n              <div className=\"space-y-3\">\n                <button className=\"w-full bg-gradient-to-r from-orange-600 to-amber-600 text-white py-4 rounded-lg font-bold text-lg shadow-lg hover:shadow-xl transition-all border-2 border-amber-900\">\n                  Create Free Account\n                </button>\n                <button \n                  onClick={() => setShowMatchModal(false)}\n                  className=\"w-full bg-white text-amber-900 py-3 rounded-lg font-semibold shadow-md hover:shadow-lg transition-all border-2 border-amber-800\"\n                >\n                  Maybe Later\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n      )}\n    </div>\n  );\n};",
