  "code": "const BoardGameConnect = () => {\n  const [location, setLocation] = useState('');\n  const [activeTab, setActiveTab] = useState('nearby');\n\n  const upcomingGames = [\n    {\n      id: 1,\n      title: 'Settlers of Catan Night',\n      host: 'Sarah Chen',\n      distance: '0.8 miles away',\n      players: '3/4 players',\n      time: 'Tonight, 7:00 PM',\n      image: 'https://picsum.photos/id/244/400/300'\n    },\n    {\n      id: 2,\n      title: 'Dungeons & Dragons Campaign',\n      host: 'Mike Rodriguez',\n      distance: '1.2 miles away',\n      players: '4/6 players',\n      time: 'Saturday, 2:00 PM',\n      image: 'https://picsum.photos/id/274/400/300'\n    },\n    {\n      id: 3,\n      title: 'Board Game Cafe Meetup',\n      host: 'Emma Wilson',\n      distance: '2.1 miles away',\n      players: '8/12 players',\n      time: 'Sunday, 4:00 PM',\n      image: 'https://picsum.photos/id/367/400/300'\n    }\n  ];\n\n  return (\n    <div className=\"min-h-screen bg-gradient-to-br from-amber-50 via-green-50 to-teal-50\">\n      {/* Hero Section */}\n      <div className=\"relative overflow-hidden\">\n        <div className=\"absolute inset-0 opacity-10\">\n          <div className=\"absolute top-20 left-10 w-64 h-64 bg-green-400 rounded-full blur-3xl\"></div>\n          <div className=\"absolute bottom-20 right-10 w-96 h-96 bg-amber-400 rounded-full blur-3xl\"></div>\n        </div>\n        \n        <div className=\"relative max-w-7xl mx-auto px-4 py-16\">\n          <div className=\"text-center mb-12\">\n            <h1 className=\"text-6xl font-bold text-gray-800 mb-4\">{{HERO_TITLE}}</h1>\n            <p className=\"text-xl text-gray-600 mb-8 max-w-2xl mx-auto\">{{DESCRIPTION}}</p>\n            \n            {/* Location Search */}\n            <div className=\"max-w-2xl mx-auto mb-8\">\n              <div className=\"flex gap-3 bg-white rounded-full shadow-lg p-2\">\n                <input\n                  type=\"text\"\n                  placeholder=\"Enter your city or zip code\"\n                  value={location}\n                  onChange={(e) => setLocation(e.target.value)}\n                  className=\"flex-1 px-6 py-3 rounded-full focus:outline-none text-gray-700\"\n                />\n                <button className=\"bg-green-600 text-white px-8 py-3 rounded-full hover:bg-green-700 transition-colors font-semibold\">{{CTA_PRIMARY}}</button>\n              </div>\n            </div>\n\n            {/* Stats */}\n            <div className=\"flex justify-center gap-8 mb-12\">\n              <div className=\"text-center\">\n                <div className=\"text-3xl font-bold text-green-700\">2,847</div>\n                <div className=\"text-sm text-gray-600\">Active Players</div>\n              </div>\n              <div className=\"text-center\">\n                <div className=\"text-3xl font-bold text-green-700\">156</div>\n                <div className=\"text-sm text-gray-600\">Games This Week</div>\n              </div>\n              <div className=\"text-center\">\n                <div className=\"text-3xl font-bold text-green-700\">42</div>\n                <div className=\"text-sm text-gray-600\">Local Groups</div>\n              </div>\n            </div>\n          </div>\n\n          {/* Tabs */}\n          <div className=\"flex justify-center gap-4 mb-8\">\n            <button\n              onClick={() =>{{CTA_SECONDARY}}</button>\n            <button\n              onClick={() =>{{CTA_1}}</button>\n            <button\n              onClick={() =>{{CTA_2}}</button>\n          </div>\n\n          {/* Game Cards */}\n          <div className=\"grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6 mb-12\">\n            {upcomingGames.map((game) => (\n              <div\n                key={game.id}\n                className=\"bg-white rounded-3xl shadow-lg overflow-hidden hover:shadow-xl transition-shadow\"\n              >\n                <img\n                  src={game.image}\n                  alt={game.title}\n                  className=\"w-full h-48 object-cover\"\n                />\n                <div className=\"p-6\">\n                  <h3 className=\"text-xl font-bold text-gray-800 mb-2\">{{FEATURE_TITLE}}</h3>\n                  <div className=\"space-y-2 mb-4\">\n                    <div className=\"flex items-center text-gray-600 text-sm\">\n                      <span className=\"w-2 h-2 bg-green-500 rounded-full mr-2\"></span>\n                      Hosted by {game.host}\n                    </div>\n                    <div className=\"flex items-center text-gray-600 text-sm\">\n                      <span className=\"w-2 h-2 bg-amber-500 rounded-full mr-2\"></span>\n                      {game.distance}\n                    </div>\n                    <div className=\"flex items-center text-gray-600 text-sm\">\n                      <span className=\"w-2 h-2 bg-teal-500 rounded-full mr-2\"></span>\n                      {game.players}\n                    </div>\n                    <div className=\"flex items-center text-gray-600 text-sm font-semibold\">\n                      <span className=\"w-2 h-2 bg-blue-500 rounded-full mr-2\"></span>\n                      {game.time}\n                    </div>\n                  </div>\n                  <button className=\"w-full bg-green-600 text-white py-3 rounded-full hover:bg-green-700 transition-colors font-semibold\">{{CTA_3}}</button>\n                </div>\n              </div>\n            ))}\n          </div>\n\n          {/* CTA Section */}\n          <div className=\"bg-white rounded-3xl shadow-xl p-8 md:p-12 text-center\">\n            <h2 className=\"text-3xl font-bold text-gray-800 mb-4\">{{FEATURE_1_TITLE}}</h2>\n            <p className=\"text-gray-600 mb-6 max-w-2xl mx-auto\">{{DESCRIPTION_1}}</p>\n            <div className=\"flex justify-center gap-4 flex-wrap\">\n              <button className=\"bg-green-600 text-white px-8 py-4 rounded-full hover:bg-green-700 transition-colors font-semibold text-lg\">{{CTA_4}}</button>\n              <button className=\"bg-gray-100 text-gray-700 px-8 py-4 rounded-full hover:bg-gray-200 transition-colors font-semibold text-lg\">{{CTA_5}}</button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n};",
