  "code": "const CoffeeChatHub = () => {\n  const [location, setLocation] = useState('');\n  const [selectedInterest, setSelectedInterest] = useState(null);\n\n  const interests = [\n    'Entrepreneurship',\n    'Tech & Innovation',\n    'Creative Arts',\n    'Career Growth',\n    'Life Coaching',\n    'Networking'\n  ];\n\n  const upcomingMeetups = [\n    {\n      id: 1,\n      name: 'Sarah Chen',\n      interest: 'Tech & Innovation',\n      location: 'Downtown Cafe',\n      time: 'Tomorrow, 2:00 PM',\n      bio: 'Software engineer passionate about AI and sustainable tech',\n      image: 'https://picsum.photos/id/64/400/400'\n    },\n    {\n      id: 2,\n      name: 'Marcus Johnson',\n      interest: 'Entrepreneurship',\n      location: 'Riverside Coffee House',\n      time: 'Friday, 10:00 AM',\n      bio: 'Startup founder looking to connect with fellow entrepreneurs',\n      image: 'https://picsum.photos/id/91/400/400'\n    },\n    {\n      id: 3,\n      name: 'Elena Rodriguez',\n      interest: 'Creative Arts',\n      location: 'Artisan Brew',\n      time: 'Saturday, 3:30 PM',\n      bio: 'Graphic designer exploring new creative collaborations',\n      image: 'https://picsum.photos/id/177/400/400'\n    }\n  ];\n\n  return (\n    <div className=\"min-h-screen bg-gradient-to-br from-amber-50 via-stone-50 to-green-50\">\n      {/* Hero Section */}\n      <div className=\"relative overflow-hidden\">\n        <div className=\"absolute inset-0 bg-gradient-to-b from-transparent to-stone-50 z-10\"></div>\n        <img \n          src=\"https://picsum.photos/id/30/1920/800\" \n          alt=\"Coffee connection\" \n          className=\"w-full h-96 object-cover\"\n        />\n        <div className=\"absolute inset-0 z-20 flex items-center justify-center\">\n          <div className=\"max-w-4xl mx-auto px-6 text-center\">\n            <h1 className=\"text-6xl font-bold text-white mb-6 drop-shadow-lg\">{{HERO_TITLE}}</h1>\n            <p className=\"text-2xl text-white mb-8 drop-shadow-md\">{{DESCRIPTION}}</p>\n            <div className=\"flex flex-col sm:flex-row gap-4 justify-center items-center max-w-2xl mx-auto\">\n              <input\n                type=\"text\"\n                placeholder=\"Enter your city or zip code\"\n                value={location}\n                onChange={(e) => setLocation(e.target.value)}\n                className=\"w-full sm:flex-1 px-6 py-4 rounded-full text-lg focus:outline-none focus:ring-4 focus:ring-amber-300 shadow-lg\"\n              />\n              <button className=\"w-full sm:w-auto bg-amber-600 hover:bg-amber-700 text-white px-10 py-4 rounded-full text-lg font-semibold shadow-lg transition-all transform hover:scale-105\">{{CTA_PRIMARY}}</button>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      {/* Interests Section */}\n      <div className=\"max-w-7xl mx-auto px-6 py-16\">\n        <h2 className=\"text-4xl font-bold text-stone-800 mb-4 text-center\">{{FEATURE_TITLE}}</h2>\n        <p className=\"text-xl text-stone-600 mb-10 text-center\">{{DESCRIPTION_1}}</p>\n        <div className=\"grid grid-cols-2 md:grid-cols-3 lg:grid-cols-6 gap-4\">\n          {interests.map((interest, index) => (\n            <button\n              key={index}\n              onClick={() => setSelectedInterest(interest)}\n              className={`px-6 py-4 rounded-2xl font-medium transition-all transform hover:scale-105 ${\n                selectedInterest === interest\n                  ? 'bg-amber-600 text-white shadow-lg'\n                  : 'bg-white text-stone-700 hover:bg-amber-50 shadow-md'\n              }`}\n            >\n              {interest}\n            </button>\n          ))}\n        </div>\n      </div>\n\n      {/* Upcoming Meetups */}\n      <div className=\"max-w-7xl mx-auto px-6 pb-16\">\n        <h2 className=\"text-4xl font-bold text-stone-800 mb-4\">{{FEATURE_1_TITLE}}</h2>\n        <p className=\"text-xl text-stone-600 mb-10\">{{DESCRIPTION_2}}</p>\n        <div className=\"grid md:grid-cols-2 lg:grid-cols-3 gap-8\">\n          {upcomingMeetups.map((meetup) => (\n            <div\n              key={meetup.id}\n              className=\"bg-white rounded-3xl overflow-hidden shadow-lg hover:shadow-2xl transition-all transform hover:-translate-y-2\"\n            >\n              <img\n                src={meetup.image}\n                alt={meetup.name}\n                className=\"w-full h-64 object-cover\"\n              />\n              <div className=\"p-6 space-y-4\">\n                <div className=\"flex items-center justify-between\">\n                  <h3 className=\"text-2xl font-bold text-stone-800\">{{FEATURE_2_TITLE}}</h3>\n                  <span className=\"px-4 py-1 bg-green-100 text-green-700 rounded-full text-sm font-medium\">\n                    Available\n                  </span>\n                </div>\n                <p className=\"text-stone-600 leading-relaxed\">\n                  {meetup.bio}\n                </p>\n                <div className=\"space-y-2 pt-4 border-t border-stone-200\">\n                  <div className=\"flex items-center gap-3 text-stone-700\">\n                    <svg className=\"w-5 h-5 text-amber-600\" fill=\"none\" stroke=\"currentColor\" viewBox=\"0 0 24 24\">\n                      <path strokeLinecap=\"round\" strokeLinejoin=\"round\" strokeWidth=\"2\" d=\"M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z\" />\n                    </svg>\n                    <span>{meetup.time}</span>\n                  </div>\n                  <div className=\"flex items-center gap-3 text-stone-700\">\n                    <svg className=\"w-5 h-5 text-amber-600\" fill=\"none\" stroke=\"currentColor\" viewBox=\"0 0 24 24\">\n                      <path strokeLinecap=\"round\" strokeLinejoin=\"round\" strokeWidth=\"2\" d=\"M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z\" />\n                      <path strokeLinecap=\"round\" strokeLinejoin=\"round\" strokeWidth=\"2\" d=\"M15 11a3 3 0 11-6 0 3 3 0 016 0z\" />\n                    </svg>\n                    <span>{meetup.location}</span>\n                  </div>\n                  <div className=\"flex items-center gap-3 text-stone-700\">\n                    <svg className=\"w-5 h-5 text-amber-600\" fill=\"none\" stroke=\"currentColor\" viewBox=\"0 0 24 24\">\n                      <path strokeLinecap=\"round\" strokeLinejoin=\"round\" strokeWidth=\"2\" d=\"M7 7h.01M7 3h5c.512 0 1.024.195 1.414.586l7 7a2 2 0 010 2.828l-7 7a2 2 0 01-2.828 0l-7-7A1.994 1.994 0 013 12V7a4 4 0 014-4z\" />\n                    </svg>\n                    <span>{meetup.interest}</span>\n                  </div>\n                </div>\n                <button className=\"w-full bg-amber-600 hover:bg-amber-700 text-white py-3 rounded-xl font-semibold transition-colors mt-4\">{{CTA_SECONDARY}}</button>\n              </div>\n            </div>\n          ))}\n        </div>\n      </div>\n\n      {/* How It Works */}\n      <div className=\"bg-white py-16\">\n        <div className=\"max-w-7xl mx-auto px-6\">\n          <h2 className=\"text-4xl font-bold text-stone-800 mb-4 text-center\">{{FEATURE_3_TITLE}}</h2>\n          <p className=\"text-xl text-stone-600 mb-12 text-center\">\n            Three simple steps to meaningful connections\n          </p>\n          <div className=\"grid md:grid-cols-3 gap-10\">\n            <div className=\"text-center space-y-4\">\n              <div className=\"w-20 h-20 bg-amber-100 rounded-full flex items-center justify-center mx-auto mb-6\">\n                <span className=\"text-3xl font-bold text-amber-600\">1</span>\n              </div>\n              <h3 className=\"text-2xl font-bold text-stone-800\">{{FEATURE_4_TITLE}}</h3>\n              <p className=\"text-stone-600 leading-relaxed\">{{DESCRIPTION_3}}</p>\n            </div>\n            <div className=\"text-center space-y-4\">\n              <div className=\"w-20 h-20 bg-green-100 rounded-full flex items-center justify-center mx-auto mb-6\">\n                <span className=\"text-3xl font-bold text-green-600\">2</span>\n              </div>\n              <h3 className=\"text-2xl font-bold text-stone-800\">{{FEATURE_5_TITLE}}</h3>\n              <p className=\"text-stone-600 leading-relaxed\">{{DESCRIPTION_4}}</p>\n            </div>\n            <div className=\"text-center space-y-4\">\n              <div className=\"w-20 h-20 bg-stone-200 rounded-full flex items-center justify-center mx-auto mb-6\">\n                <span className=\"text-3xl font-bold text-stone-600\">3</span>\n              </div>\n              <h3 className=\"text-2xl font-bold text-stone-800\">{{FEATURE_6_TITLE}}</h3>\n              <p className=\"text-stone-600 leading-relaxed\">{{DESCRIPTION_5}}</p>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      {/* CTA Section */}\n      <div className=\"relative overflow-hidden py-20\">\n        <img \n          src=\"https://picsum.photos/id/431/1920/600\" \n          alt=\"Coffee community\" \n          className=\"absolute inset-0 w-full h-full object-cover\"\n        />\n        <div className=\"absolute inset-0 bg-stone-900 bg-opacity-70\"></div>\n        <div className=\"relative z-10 max-w-4xl mx-auto px-6 text-center\">\n          <h2 className=\"text-5xl font-bold text-white mb-6\">{{FEATURE_7_TITLE}}</h2>\n          <p className=\"text-2xl text-stone-200 mb-10\">{{DESCRIPTION_6}}</p>\n          <button className=\"bg-amber-600 hover:bg-amber-700 text-white px-12 py-5 rounded-full text-xl font-semibold shadow-2xl transition-all transform hover:scale-105\">{{CTA_1}}</button>\n        </div>\n      </div>\n    </div>\n  );\n};",
