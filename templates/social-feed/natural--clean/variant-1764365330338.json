  "code": "const CoffeeChatHub = () => {\n  const [location, setLocation] = useState('');\n  const [showMatches, setShowMatches] = useState(false);\n\n  const featuredMembers = [\n    { id: 1, name: 'Sarah Chen', bio: 'Entrepreneur & coffee enthusiast', interests: 'Startups, Design, Travel', image: 'https://picsum.photos/id/64/400/400' },\n    { id: 2, name: 'Marcus Johnson', bio: 'Creative director seeking connections', interests: 'Art, Photography, Music', image: 'https://picsum.photos/id/91/400/400' },\n    { id: 3, name: 'Elena Rodriguez', bio: 'Writer exploring the city', interests: 'Books, Culture, Food', image: 'https://picsum.photos/id/177/400/400' },\n    { id: 4, name: 'David Park', bio: 'Tech professional, new to the area', interests: 'Coding, Hiking, Coffee', image: 'https://picsum.photos/id/338/400/400' }\n  ];\n\n  return (\n    <div className=\"min-h-screen bg-gradient-to-br from-amber-50 via-stone-50 to-green-50\">\n      {/* Hero Section */}\n      <div className=\"relative overflow-hidden\">\n        <div className=\"absolute inset-0 opacity-10\">\n          <div className=\"absolute top-20 left-10 w-64 h-64 bg-amber-400 rounded-full blur-3xl\"></div>\n          <div className=\"absolute bottom-20 right-10 w-96 h-96 bg-green-400 rounded-full blur-3xl\"></div>\n        </div>\n\n        <div className=\"relative max-w-7xl mx-auto px-6 py-16\">\n          <div className=\"grid md:grid-cols-2 gap-12 items-center mb-16\">\n            <div className=\"space-y-6\">\n              <div className=\"inline-block px-4 py-2 bg-amber-100 rounded-full text-amber-800 text-sm font-medium mb-4\">\n                â˜• Connect Locally\n              </div>\n              <h1 className=\"text-5xl md:text-6xl font-bold text-stone-900 leading-tight\">{{HERO_TITLE}}</h1>\n              <p className=\"text-xl text-stone-600 leading-relaxed\">\n                Discover like-minded people in your neighborhood. Share stories, ideas, and great coffee moments together.\n              </p>\n              \n              <div className=\"flex flex-col sm:flex-row gap-4 pt-4\">\n                <input\n                  type=\"text\"\n                  placeholder=\"Enter your city or zip code\"\n                  value={location}\n                  onChange={(e) => setLocation(e.target.value)}\n                  className=\"flex-1 px-6 py-4 rounded-2xl border-2 border-stone-200 focus:border-amber-400 focus:outline-none bg-white shadow-sm\"\n                />\n                <button\n                  onClick={() => setShowMatches(true)}\n                  className=\"px-8 py-4 bg-gradient-to-r from-amber-600 to-amber-500 text-white font-semibold rounded-2xl hover:from-amber-700 hover:to-amber-600 transition-all shadow-lg hover:shadow-xl\"\n                >\n                  Find Matches\n                </button>\n              </div>\n\n              <div className=\"flex items-center gap-8 pt-4\">\n                <div className=\"text-center\">\n                  <div className=\"text-3xl font-bold text-stone-900\">12.5K+</div>\n                  <div className=\"text-sm text-stone-600\">Active Members</div>\n                </div>\n                <div className=\"text-center\">\n                  <div className=\"text-3xl font-bold text-stone-900\">8.2K+</div>\n                  <div className=\"text-sm text-stone-600\">Coffee Chats</div>\n                </div>\n                <div className=\"text-center\">\n                  <div className=\"text-3xl font-bold text-stone-900\">250+</div>\n                  <div className=\"text-sm text-stone-600\">Cities</div>\n                </div>\n              </div>\n            </div>\n\n            <div className=\"relative\">\n              <div className=\"rounded-3xl overflow-hidden shadow-2xl\">\n                <img \n                  src=\"https://picsum.photos/id/30/800/900\" \n                  alt=\"Coffee meeting\" \n                  className=\"w-full h-full object-cover\"\n                />\n              </div>\n              <div className=\"absolute -bottom-6 -left-6 bg-white rounded-2xl shadow-xl p-6 max-w-xs\">\n                <div className=\"flex items-center gap-4\">\n                  <img \n                    src=\"https://picsum.photos/id/65/100/100\" \n                    alt=\"Member\" \n                    className=\"w-12 h-12 rounded-full object-cover\"\n                  />\n                  <div>\n                    <div className=\"font-semibold text-stone-900\">Coffee chat scheduled!</div>\n                    <div className=\"text-sm text-stone-600\">Tomorrow at 3pm</div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      {/* Featured Members Section */}\n      <div className=\"max-w-7xl mx-auto px-6 py-16\">\n        <div className=\"text-center mb-12\">\n          <h2 className=\"text-4xl font-bold text-stone-900 mb-4\">{{FEATURE_TITLE}}</h2>\n          <p className=\"text-lg text-stone-600 max-w-2xl mx-auto\">\n            Browse profiles of friendly people in your area looking to share meaningful conversations\n          </p>\n        </div>\n\n        <div className=\"grid md:grid-cols-2 lg:grid-cols-4 gap-6 mb-12\">\n          {featuredMembers.map((member) => (\n            <div key={member.id} className=\"bg-white rounded-3xl overflow-hidden shadow-lg hover:shadow-2xl transition-all hover:-translate-y-2 duration-300\">\n              <img \n                src={member.image} \n                alt={member.name} \n                className=\"w-full h-64 object-cover\"\n              />\n              <div className=\"p-6 space-y-3\">\n                <h3 className=\"text-xl font-bold text-stone-900\">{{FEATURE_1_TITLE}}</h3>\n                <p className=\"text-stone-600 text-sm\">{member.bio}</p>\n                <div className=\"flex flex-wrap gap-2\">\n                  {member.interests.split(', ').map((interest, idx) => (\n                    <span key={idx} className=\"px-3 py-1 bg-amber-50 text-amber-700 rounded-full text-xs font-medium\">\n                      {interest}\n                    </span>\n                  ))}\n                </div>\n                <button className=\"w-full mt-4 px-4 py-3 bg-stone-100 hover:bg-amber-100 text-stone-900 font-medium rounded-xl transition-colors\">{{CTA_PRIMARY}}</button>\n              </div>\n            </div>\n          ))}\n        </div>\n\n        {/* How It Works */}\n        <div className=\"bg-white rounded-3xl shadow-xl p-12 mt-16\">\n          <h3 className=\"text-3xl font-bold text-stone-900 text-center mb-12\">{{FEATURE_2_TITLE}}</h3>\n          <div className=\"grid md:grid-cols-3 gap-8\">\n            <div className=\"text-center space-y-4\">\n              <div className=\"w-16 h-16 bg-gradient-to-br from-amber-400 to-amber-500 rounded-2xl flex items-center justify-center text-white text-2xl font-bold mx-auto\">\n                1\n              </div>\n              <h4 className=\"text-xl font-bold text-stone-900\">Create Your Profile</h4>\n              <p className=\"text-stone-600\">Share your interests, favorite coffee spots, and what you'd like to chat about</p>\n            </div>\n            <div className=\"text-center space-y-4\">\n              <div className=\"w-16 h-16 bg-gradient-to-br from-green-400 to-green-500 rounded-2xl flex items-center justify-center text-white text-2xl font-bold mx-auto\">\n                2\n              </div>\n              <h4 className=\"text-xl font-bold text-stone-900\">Find Your Match</h4>\n              <p className=\"text-stone-600\">Browse local members and connect with people who share your vibe</p>\n            </div>\n            <div className=\"text-center space-y-4\">\n              <div className=\"w-16 h-16 bg-gradient-to-br from-stone-400 to-stone-500 rounded-2xl flex items-center justify-center text-white text-2xl font-bold mx-auto\"