  "code": "const BoardGameConnect = () => {\n  const [location, setLocation] = useState('');\n  const [selectedGame, setSelectedGame] = useState(null);\n\n  const featuredGames = [\n    { id: 1, name: 'Settlers of Catan', players: '3-4', image: 'https://picsum.photos/id/244/400/300', activeGroups: 12 },\n    { id: 2, name: 'Ticket to Ride', players: '2-5', image: 'https://picsum.photos/id/274/400/300', activeGroups: 8 },\n    { id: 3, name: 'Pandemic', players: '2-4', image: 'https://picsum.photos/id/367/400/300', activeGroups: 15 },\n  ];\n\n  const upcomingMeetups = [\n    { id: 1, title: 'Weekend Strategy Games', location: 'Green Park Cafe', date: 'Sat, 2:00 PM', attendees: 6, image: 'https://picsum.photos/id/0/200/200' },\n    { id: 2, title: 'Family Board Game Night', location: 'Community Center', date: 'Sun, 4:00 PM', attendees: 12, image: 'https://picsum.photos/id/180/200/200' },\n  ];\n\n  return (\n    <div className=\"min-h-screen bg-gradient-to-br from-amber-50 via-green-50 to-teal-50\">\n      {/* Hero Section */}\n      <div className=\"max-w-7xl mx-auto px-4 py-16\">\n        <div className=\"text-center mb-16\">\n          <h1 className=\"text-6xl font-bold text-gray-800 mb-4\">{{HERO_TITLE}}</h1>\n          <p className=\"text-2xl text-gray-600 mb-8\">{{DESCRIPTION}}</p>\n          \n          {/* Search Bar */}\n          <div className=\"max-w-2xl mx-auto bg-white rounded-full shadow-lg p-2 flex gap-2\">\n            <input\n              type=\"text\"\n              placeholder=\"Enter your city or zip code\"\n              value={location}\n              onChange={(e) => setLocation(e.target.value)}\n              className=\"flex-1 px-6 py-3 rounded-full focus:outline-none text-gray-700\"\n            />\n            <button className=\"bg-green-600 text-white px-8 py-3 rounded-full hover:bg-green-700 transition-colors font-semibold\">\n              Find Games\n            </button>\n          </div>\n        </div>\n\n        {/* Featured Games */}\n        <div className=\"mb-16\">\n          <h2 className=\"text-3xl font-bold text-gray-800 mb-8 text-center\">{{FEATURE_TITLE}}</h2>\n          <div className=\"grid md:grid-cols-3 gap-8\">\n            {featuredGames.map((game) => (\n              <div\n                key={game.id}\n                onClick={() => setSelectedGame(game.id)}\n                className={`bg-white rounded-2xl overflow-hidden shadow-md hover:shadow-xl transition-all cursor-pointer transform hover:-translate-y-1 ${\n                  selectedGame === game.id ? 'ring-4 ring-green-500' : ''\n                }`}\n              >\n                <img\n                  src={game.image}\n                  alt={game.name}\n                  className=\"w-full h-48 object-cover\"\n                />\n                <div className=\"p-6\">\n                  <h3 className=\"text-xl font-bold text-gray-800 mb-2\">{{FEATURE_1_TITLE}}</h3>\n                  <div className=\"flex justify-between items-center text-sm text-gray-600 mb-4\">\n                    <span className=\"flex items-center gap-1\">\n                      <svg className=\"w-4 h-4\" fill=\"currentColor\" viewBox=\"0 0 20 20\">\n                        <path d=\"M9 6a3 3 0 11-6 0 3 3 0 016 0zM17 6a3 3 0 11-6 0 3 3 0 016 0zM12.93 17c.046-.327.07-.66.07-1a6.97 6.97 0 00-1.5-4.33A5 5 0 0119 16v1h-6.07zM6 11a5 5 0 015 5v1H1v-1a5 5 0 015-5z\" />\n                      </svg>\n                      {game.players} players\n                    </span>\n                    <span className=\"bg-green-100 text-green-700 px-3 py-1 rounded-full font-semibold\">\n                      {game.activeGroups} groups\n                    </span>\n                  </div>\n                  <button className=\"w-full bg-amber-100 text-amber-800 py-2 rounded-lg hover:bg-amber-200 transition-colors font-semibold\">{{CTA_PRIMARY}}</button>\n                </div>\n              </div>\n            ))}\n          </div>\n        </div>\n\n        {/* Upcoming Meetups */}\n        <div className=\"mb-16\">\n          <h2 className=\"text-3xl font-bold text-gray-800 mb-8 text-center\">{{FEATURE_2_TITLE}}</h2>\n          <div className=\"grid md:grid-cols-2 gap-6\">\n            {upcomingMeetups.map((meetup) => (\n              <div key={meetup.id} className=\"bg-white rounded-2xl p-6 shadow-md hover:shadow-lg transition-shadow flex gap-6\">\n                <img\n                  src={meetup.image}\n                  alt={meetup.title}\n                  className=\"w-24 h-24 rounded-xl object-cover\"\n                />\n                <div className=\"flex-1\">\n                  <h3 className=\"text-xl font-bold text-gray-800 mb-2\">{{FEATURE_3_TITLE}}</h3>\n                  <div className=\"space-y-1 text-gray-600 mb-4\">\n                    <p className=\"flex items-center gap-2\">\n                      <svg className=\"w-4 h-4\" fill=\"currentColor\" viewBox=\"0 0 20 20\">\n                        <path fillRule=\"evenodd\" d=\"M5.05 4.05a7 7 0 119.9 9.9L10 18.9l-4.95-4.95a7 7 0 010-9.9zM10 11a2 2 0 100-4 2 2 0 000 4z\" clipRule=\"evenodd\" />\n                      </svg>\n                      {meetup.location}\n                    </p>\n                    <p className=\"flex items-center gap-2\">\n                      <svg className=\"w-4 h-4\" fill=\"currentColor\" viewBox=\"0 0 20 20\">\n                        <path fillRule=\"evenodd\" d=\"M6 2a1 1 0 00-1 1v1H4a2 2 0 00-2 2v10a2 2 0 002 2h12a2 2 0 002-2V6a2 2 0 00-2-2h-1V3a1 1 0 10-2 0v1H7V3a1 1 0 00-1-1zm0 5a1 1 0 000 2h8a1 1 0 100-2H6z\" clipRule=\"evenodd\" />\n                      </svg>\n                      {meetup.date}\n                    </p>\n                  </div>\n                  <div className=\"flex items-center justify-between\">\n                    <span className=\"text-sm text-gray-600\">{meetup.attendees} attending</span>\n                    <button className=\"bg-teal-600 text-white px-6 py-2 rounded-lg hover:bg-teal-700 transition-colors font-semibold\">\n                      RSVP\n                    </button>\n                  </div>\n                </div>\n              </div>\n            ))}\n          </div>\n        </div>\n\n        {/* CTA Section */}\n        <div className=\"bg-gradient-to-r from-green-600 to-teal-600 rounded-3xl p-12 text-center text-white shadow-xl\">\n          <h2 className=\"text-4xl font-bold mb-4\">{{FEATURE_4_TITLE}}</h2>\n          <p className=\"text-xl mb-8 opacity-90\">{{DESCRIPTION_1}}</p>\n          <div className=\"flex gap-4 justify-center flex-wrap\">\n            <button className=\"bg-white text-green-700 px-8 py-4 rounded-full font-bold text-lg hover:bg-gray-100 transition-colors shadow-lg\">\n              Create Account\n            </button>\n            <button className=\"bg-transparent border-2 border-white text-white px-8 py-4 rounded-full font-bold text-lg hover:bg-white hover:text-green-700 transition-colors\">\n              Host a Game\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n};",
