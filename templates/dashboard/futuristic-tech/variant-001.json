  "code": "const AnalyticsDashboard = () => {\n  const [timeRange, setTimeRange] = useState('7d');\n  const [selectedMetric, setSelectedMetric] = useState('users');\n\n  const metrics = [\n    { label: 'Active Users', value: '47.2K', change: '+12.5%', trend: 'up' },\n    { label: 'Conversion Rate', value: '3.84%', change: '+0.8%', trend: 'up' },\n    { label: 'Avg. Session', value: '4m 32s', change: '-5.2%', trend: 'down' },\n    { label: 'Revenue', value: '$128.4K', change: '+18.3%', trend: 'up' }\n  ];\n\n  const funnelSteps = [\n    { step: 'Landing Page', users: 10000, percentage: 100, color: 'from-cyan-500 to-blue-500' },\n    { step: 'Sign Up Started', users: 7500, percentage: 75, color: 'from-blue-500 to-purple-500' },\n    { step: 'Profile Created', users: 5250, percentage: 52.5, color: 'from-purple-500 to-pink-500' },\n    { step: 'First Action', users: 3150, percentage: 31.5, color: 'from-pink-500 to-rose-500' },\n    { step: 'Converted', users: 1890, percentage: 18.9, color: 'from-rose-500 to-orange-500' }\n  ];\n\n  const engagementData = [\n    { day: 'Mon', value: 65 },\n    { day: 'Tue', value: 78 },\n    { day: 'Wed', value: 72 },\n    { day: 'Thu', value: 85 },\n    { day: 'Fri', value: 92 },\n    { day: 'Sat', value: 58 },\n    { day: 'Sun', value: 48 }\n  ];\n\n  return (\n    <div className=\"min-h-screen bg-gradient-to-br from-slate-950 via-purple-950 to-slate-950 text-white\">\n      <div className=\"absolute inset-0 bg-[url('data:image/svg+xml;base64,')] opacity-40\"></div>\n      \n      <div className=\"relative z-10 max-w-7xl mx-auto px-6 py-8\">\n        <div className=\"mb-8\">\n          <div className=\"flex items-center justify-between mb-6\">\n            <div>\n              <h1 className=\"text-4xl font-bold bg-gradient-to-r from-cyan-400 via-purple-400 to-pink-400 bg-clip-text text-transparent mb-2\">\n                Analytics Command Center\n              </h1>\n              <p className=\"text-slate-400\">Real-time insights and conversion tracking</p>\n            </div>\n            <div className=\"flex gap-3\">\n              {['24h', '7d', '30d', '90d'].map((range) => (\n                <button\n                  key={range}\n                  onClick={() => setTimeRange(range)}\n                  className={`px-4 py-2 rounded-lg font-medium transition-all ${\n                    timeRange === range\n                      ? 'bg-gradient-to-r from-cyan-500 to-purple-500 text-white shadow-lg shadow-purple-500/50'\n                      : 'bg-white/5 backdrop-blur-sm text-slate-400 hover:bg-white/10 border border-white/10'\n                  }`}\n                >\n                  {range}\n                </button>\n              ))}\n            </div>\n          </div>\n\n          <div className=\"grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-6 mb-8\">\n            {metrics.map((metric, idx) => (\n              <div\n                key={idx}\n                className=\"bg-white/5 backdrop-blur-xl border border-white/10 rounded-2xl p-6 hover:bg-white/10 transition-all hover:shadow-2xl hover:shadow-purple-500/20 cursor-pointer group\"\n              >\n                <div className=\"flex items-start justify-between mb-4\">\n                  <div className=\"w-12 h-12 rounded-xl bg-gradient-to-br from-cyan-500/20 to-purple-500/20 flex items-center justify-center group-hover:scale-110 transition-transform\">\n                    <div className=\"w-6 h-6 rounded-lg bg-gradient-to-br from-cyan-400 to-purple-400\"></div>\n                  </div>\n                  <span className={`px-3 py-1 rounded-full text-xs font-medium ${\n                    metric.trend === 'up' \n                      ? 'bg-emerald-500/20 text-emerald-400 border border-emerald-500/30' \n                      : 'bg-rose-500/20 text-rose-400 border border-rose-500/30'\n                  }`}>\n                    {metric.change}\n                  </span>\n                </div>\n                <h3 className=\"text-slate-400 text-sm mb-2\">{metric.label}</h3>\n                <p className=\"text-3xl font-bold bg-gradient-to-r from-white to-slate-300 bg-clip-text text-transparent\">\n                  {metric.value}\n                </p>\n              </div>\n            ))}\n          </div>\n        </div>\n\n        <div className=\"grid grid-cols-1 lg:grid-cols-3 gap-6 mb-8\">\n          <div className=\"lg:col-span-2 bg-white/5 backdrop-blur-xl border border-white/10 rounded-2xl p-6\">\n            <div className=\"flex items-center justify-between mb-6\">\n              <h2 className=\"text-xl font-bold text-white\">User Engagement Trend</h2>\n              <div className=\"flex gap-2\">\n                <button className=\"px-3 py-1 rounded-lg bg-purple-500/20 text-purple-300 text-sm font-medium border border-purple-500/30\">\n                  Users\n                </button>\n                <button className=\"px-3 py-1 rounded-lg bg-white/5 text-slate-400 text-sm font-medium hover:bg-white/10 transition-all\">\n                  Sessions\n                </button>\n              </div>\n            </div>\n            <div className=\"flex items-end justify-between h-64 gap-4\">\n              {engagementData.map((data, idx) => (\n                <div key={idx} className=\"flex-1 flex flex-col items-center gap-3\">\n                  <div className=\"w-full bg-gradient-to-t from-cyan-500 via-purple-500 to-pink-500 rounded-t-lg relative group cursor-pointer hover:opacity-80 transition-opacity\" \n                       style={{ height: `${data.value}%` }}>\n                    <div className=\"absolute -top-8 left-1/2 -translate-x-1/2 bg-slate-900 px-2 py-1 rounded text-xs font-medium opacity-0 group-hover:opacity-100 transition-opacity whitespace-nowrap\">\n                      {data.value}%\n                    </div>\n                  </div>\n                  <span className=\"text-slate-400 text-sm font-medium\">{data.day}</span>\n                </div>\n              ))}\n            </div>\n          </div>\n\n          <div className=\"bg-white/5 backdrop-blur-xl border border-white/10 rounded-2xl p-6\">\n            <h2 className=\"text-xl font-bold text-white mb-6\">Top Channels</h2>\n            <div className=\"space-y-4\">\n              {[\n                { name: 'Organic Search', value: 42, color: 'from-cyan-500 to-blue-500' },\n                { name: 'Direct', value: 28, color: 'from-purple-500 to-pink-500' },\n                { name: 'Social Media', value: 18, color: 'from-pink-500 to-rose-500' },\n                { name: 'Email', value: 12, color: 'from-orange-500 to-yellow-500' }\n              ].map((channel, idx) => (\n                <div key={idx} className=\"space-y-2\">\n                  <div className=\"flex items-center justify-between text-sm\">\n                    <span className=\"text-slate-300 font-medium\">{channel.name}</span>\n                    <span className=\"text-white font-bold\">{channel.value}%</span>\n                  </div>\n                  <div className=\"w-full h-2 bg-white/5 rounded-full overflow-hidden\">\n                    <div \n                      className={`h-full bg-gradient-to-r ${channel.color} rounded-full transition-all duration-1000`}\n                      style={{ width: `${channel.value}%` }}\n                    ></div>\n                  </div>\n                </div>\n              ))}\n            </div>\n          </div>\n        </div>\n\n        <div className=\"bg-white/5 backdrop-blur-xl border border-white/10 rounded-2xl p-6\">\n          <h2 className=\"text-2xl font-bold text-white mb-2\">Conversion Funnel Analysis</h2>\n          <p className=\"text-slate-400 mb-8\">Track user journey from landing to conversion</p>\n          \n          <div className=\"space-y-4\">\n            {funnelSteps.map((step, idx) => (\n              <div key={idx} className=\"relative\">\n                <div className=\"flex items-center gap-6\">\n                  <div className=\"w-48 text-right\">\n                    <p className=\"text-white font-medium\">{step.step}</p>\n                    <p className=\"text-slate-400 text-sm\">{step.users.toLocaleString()} users</p>\n                  </div>\n                  \n                  <div className=\"flex-1 relative\">\n                    <div className=\"h-16 bg-white/5 rounded-lg overflow-hidden\">\n                      <div \n                        className={`h-full bg-gradient-to-r ${step.color} relative group cursor-pointer transition-all hover:opacity-90`}\n                        style={{ width: `${step.percentage}%` }}\n                      >\n                        <div className=\"absolute inset-0 bg-white/10 opacity-0 group-hover:opacity-100 transition-opacity\"></div>\n                      </div>\n                    </div>\n                    <div className=\"absolute right-4 top-1/2 -translate-y-1/2 text-white font-bold text-lg\">\n                      {step.percentage}%\n                    </div>\n                  </div>\n\n                  <div className=\"w-24 text-left\">\n                    {idx < funnelSteps.length - 1 && (\n                      <span className=\"text-rose-400 text-sm font-medium\">\n                        -{((funnelSteps[idx].percentage - funnelSteps[idx + 1].percentage).toFixed(1))}%\n                      </span>\n                    )}\n                  </div>\n                </div>\n                \n                {idx < funnelSteps.length - 1 && (\n                  <div className=\"ml-48 pl-6 mt-2\">\n                    <div className=\"w-px h-4 bg-gradient-to-b from-white/20 to-transparent ml-4\"></div>\n                  </div>\n                )}\n              </div>\n            ))}\n          </div>\n\n          <div className=\"mt-8 p-4 bg-gradient-to-r from-emerald-500/10 to-cyan-500/10 border border-emerald-500/20 rounded-xl\">\n            <div className=\"flex items-center gap-3\">\n              <div className=\"w-10 h-10 rounded-full bg-emerald-500/20 flex items-center justify-center\">\n                <span className=\"text-emerald-400 text-xl\">âœ“</span>\n              </div>\n              <div>\n                <p className=\"text-white font-medium\">Overall Conversion Rate: 18.9%</p>\n                <p className=\"text-slate-400 text-sm\">+2.3% improvement from last period</p>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n};",
