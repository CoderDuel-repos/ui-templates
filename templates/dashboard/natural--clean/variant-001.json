  "code": "const AnalyticsDashboard = () => {\n  const [timeRange, setTimeRange] = useState('7d');\n  const [selectedMetric, setSelectedMetric] = useState('users');\n\n  const metrics = [\n    { label: 'Active Users', value: '12,453', change: '+12.5%', positive: true },\n    { label: 'Conversion Rate', value: '3.42%', change: '+0.8%', positive: true },\n    { label: 'Avg. Session', value: '4m 32s', change: '-0.3%', positive: false },\n    { label: 'Revenue', value: '$24,891', change: '+18.2%', positive: true }\n  ];\n\n  const funnelSteps = [\n    { name: 'Landing Page', users: 10000, percentage: 100, color: 'bg-emerald-500' },\n    { name: 'Sign Up Started', users: 6500, percentage: 65, color: 'bg-emerald-400' },\n    { name: 'Form Completed', users: 4200, percentage: 42, color: 'bg-emerald-300' },\n    { name: 'Email Verified', users: 3100, percentage: 31, color: 'bg-emerald-200' },\n    { name: 'First Purchase', users: 1850, percentage: 18.5, color: 'bg-emerald-100' }\n  ];\n\n  const engagementData = [\n    { day: 'Mon', value: 65 },\n    { day: 'Tue', value: 72 },\n    { day: 'Wed', value: 68 },\n    { day: 'Thu', value: 85 },\n    { day: 'Fri', value: 78 },\n    { day: 'Sat', value: 45 },\n    { day: 'Sun', value: 38 }\n  ];\n\n  const topPages = [\n    { page: '/dashboard', views: 8234, icon: 'üè†' },\n    { page: '/products', views: 6821, icon: 'üõçÔ∏è' },\n    { page: '/pricing', views: 5432, icon: 'üí∞' },\n    { page: '/about', views: 3210, icon: '‚ÑπÔ∏è' }\n  ];\n\n  return (\n    <div className=\"min-h-screen bg-gradient-to-br from-stone-50 via-amber-50 to-emerald-50\">\n      <div className=\"max-w-7xl mx-auto px-6 py-8\">\n        \n        {/* Header */}\n        <div className=\"mb-8\">\n          <div className=\"flex items-center justify-between mb-6\">\n            <div>\n              <h1 className=\"text-4xl font-bold text-stone-800 mb-2\">Analytics Overview</h1>\n              <p className=\"text-stone-600\">Track your growth with natural insights</p>\n            </div>\n            <div className=\"flex items-center gap-3\">\n              <select \n                value={timeRange}\n                onChange={(e) => setTimeRange(e.target.value)}\n                className=\"px-4 py-2 bg-white border-2 border-stone-200 rounded-xl text-stone-700 focus:outline-none focus:border-emerald-400 transition-colors\"\n              >\n                <option value=\"24h\">Last 24 Hours</option>\n                <option value=\"7d\">Last 7 Days</option>\n                <option value=\"30d\">Last 30 Days</option>\n                <option value=\"90d\">Last 90 Days</option>\n              </select>\n              <button className=\"px-6 py-2 bg-emerald-500 text-white rounded-xl hover:bg-emerald-600 transition-colors shadow-lg shadow-emerald-200\">\n                Export Report\n              </button>\n            </div>\n          </div>\n        </div>\n\n        {/* Key Metrics Cards */}\n        <div className=\"grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-6 mb-8\">\n          {metrics.map((metric, index) => (\n            <div key={index} className=\"bg-white rounded-2xl p-6 shadow-lg shadow-stone-200 border border-stone-100 hover:shadow-xl transition-shadow\">\n              <div className=\"flex items-start justify-between mb-4\">\n                <span className=\"text-stone-500 text-sm font-medium\">{metric.label}</span>\n                <span className={`px-3 py-1 rounded-full text-xs font-semibold ${\n                  metric.positive ? 'bg-emerald-100 text-emerald-700' : 'bg-amber-100 text-amber-700'\n                }`}>\n                  {metric.change}\n                </span>\n              </div>\n              <div className=\"text-3xl font-bold text-stone-800\">{metric.value}</div>\n            </div>\n          ))}\n        </div>\n\n        <div className=\"grid grid-cols-1 lg:grid-cols-3 gap-6 mb-8\">\n          \n          {/* Engagement Chart */}\n          <div className=\"lg:col-span-2 bg-white rounded-2xl p-6 shadow-lg shadow-stone-200 border border-stone-100\">\n            <div className=\"flex items-center justify-between mb-6\">\n              <h2 className=\"text-xl font-bold text-stone-800\">User Engagement</h2>\n              <div className=\"flex gap-2\">\n                <button \n                  onClick={() => setSelectedMetric('users')}\n                  className={`px-4 py-2 rounded-lg text-sm font-medium transition-colors ${\n                    selectedMetric === 'users' \n                      ? 'bg-emerald-500 text-white' \n                      : 'bg-stone-100 text-stone-600 hover:bg-stone-200'\n                  }`}\n                >\n                  Users\n                </button>\n                <button \n                  onClick={() => setSelectedMetric('sessions')}\n                  className={`px-4 py-2 rounded-lg text-sm font-medium transition-colors ${\n                    selectedMetric === 'sessions' \n                      ? 'bg-emerald-500 text-white' \n                      : 'bg-stone-100 text-stone-600 hover:bg-stone-200'\n                  }`}\n                >\n                  Sessions\n                </button>\n              </div>\n            </div>\n            \n            <div className=\"flex items-end justify-between h-64 gap-4\">\n              {engagementData.map((data, index) => (\n                <div key={index} className=\"flex-1 flex flex-col items-center gap-2\">\n                  <div className=\"w-full bg-stone-100 rounded-t-xl overflow-hidden relative\" style={{ height: '100%' }}>\n                    <div \n                      className=\"absolute bottom-0 w-full bg-gradient-to-t from-emerald-500 to-emerald-300 rounded-t-xl transition-all duration-500 hover:from-emerald-600 hover:to-emerald-400\"\n                      style={{ height: `${data.value}%` }}\n                    />\n                  </div>\n                  <span className=\"text-sm font-medium text-stone-600\">{data.day}</span>\n                </div>\n              ))}\n            </div>\n          </div>\n\n          {/* Top Pages */}\n          <div className=\"bg-white rounded-2xl p-6 shadow-lg shadow-stone-200 border border-stone-100\">\n            <h2 className=\"text-xl font-bold text-stone-800 mb-6\">Top Pages</h2>\n            <div className=\"space-y-4\">\n              {topPages.map((page, index) => (\n                <div key={index} className=\"flex items-center gap-4 p-3 rounded-xl hover:bg-stone-50 transition-colors\">\n                  <span className=\"text-2xl\">{page.icon}</span>\n                  <div className=\"flex-1\">\n                    <div className=\"text-sm font-medium text-stone-800\">{page.page}</div>\n                    <div className=\"text-xs text-stone-500\">{page.views.toLocaleString()} views</div>\n                  </div>\n                  <div className=\"w-16 h-2 bg-stone-100 rounded-full overflow-hidden\">\n                    <div \n                      className=\"h-full bg-emerald-500 rounded-full\"\n                      style={{ width: `${(page.views / topPages[0].views) * 100}%` }}\n                    />\n                  </div>\n                </div>\n              ))}\n            </div>\n          </div>\n        </div>\n\n        {/* Conversion Funnel */}\n        <div className=\"bg-white rounded-2xl p-8 shadow-lg shadow-stone-200 border border-stone-100 mb-8\">\n          <div className=\"flex items-center justify-between mb-8\">\n            <div>\n              <h2 className=\"text-2xl font-bold text-stone-800 mb-2\">Conversion Funnel</h2>\n              <p className=\"text-stone-600\">Track user journey from landing to conversion</p>\n            </div>\n            <div className=\"text-right\">\n              <div className=\"text-3xl font-bold text-emerald-600\">18.5%</div>\n              <div className=\"text-sm text-stone-500\">Overall Conversion</div>\n            </div>\n          </div>\n\n          <div className=\"space-y-4\">\n            {funnelSteps.map((step, index) => (\n              <div key={index} className=\"relative\">\n                <div className=\"flex items-center gap-6\">\n                  <div className=\"w-32 text-right\">\n                    <div className=\"text-sm font-semibold text-stone-800\">{step.name}</div>\n                    <div className=\"text-xs text-stone-500\">{step.users.toLocaleString()} users</div>\n                  </div>\n                  \n                  <div className=\"flex-1 relative\">\n                    <div className=\"h-16 bg-stone-100 rounded-xl overflow-hidden\">\n                      <div \n                        className={`h-full ${step.color} transition-all duration-700 flex items-center px-6 rounded-xl`}\n                        style={{ width: `${step.percentage}%` }}\n                      >\n                        <span className=\"text-stone-800 font-bold text-lg\">{step.percentage}%</span>\n                      </div>\n                    </div>\n                  </div>\n\n                  {index < funnelSteps.length - 1 && (\n                    <div className=\"text-sm font-medium text-stone-500 w-20\">\n                      -{((funnelSteps[index].users - funnelSteps[index + 1].users) / funnelSteps[index].users * 100).toFixed(1)}%\n                    </div>\n                  )}\n                </div>\n              </div>\n            ))}\n          </div>\n        </div>\n\n        {/* User Insights */}\n        <div className=\"grid grid-cols-1 md:grid-cols-3 gap-6\">\n          <div className=\"bg-gradient-to-br from-emerald-500 to-emerald-600 rounded-2xl p-6 text-white shadow-lg shadow-emerald-200\">\n            <div className=\"text-4xl mb-4\">üå±</div>\n            <h3 className=\"text-xl font-bold mb-2\">Growing Steadily</h3>\n            <p className=\"text-emerald-100 text-sm mb-4\">User base increased by 23% this month</p>\n            <button className=\"px-4 py-2 bg-white text-emerald-600 rounded-lg font-medium hover:bg-emerald-50 transition-colors\">\n              View Details\n            </button>\n          </div>\n\n          <div className=\"bg-gradient-to-br from-amber-500 to-amber-600 rounded-2xl p-6 text-white shadow-lg shadow-amber-200\">\n            <div className=\"text-4xl mb-4\">‚ö°</div>\n            <h3 className=\"text-xl font-bold mb-2\">Peak Hours</h3>\n            <p className=\"text-amber-100 text-sm mb-4\">Most activity between 2-5 PM daily</p>\n            <button className=\"px-4 py-2 bg-white text-amber-600 rounded-lg font-medium hover:bg-amber-50 transition-colors\">\n              Optimize\n            </button>\n          </div>\n\n          <div className=\"bg-gradient-to-br from-stone-600 to-stone-700 rounded-2xl p-6 text-white shadow-lg shadow-stone-300\">\n            <div className=\"text-4xl mb-4\">üéØ</div>\n            <h3 className=\"text-xl font-bold mb-2\">Key Opportunities</h3>\n            <p className=\"text-stone-300 text-sm mb-4\">3 areas identified for improvement</p>\n            <button className=\"px-4 py-2 bg-white text-stone-700 rounded-lg font-medium hover:bg-stone-50 transition-colors\">\n              Explore\n            </button>\n          </div>\n        </div>\n\n      </div>\n    </div>\n  );\n};",
