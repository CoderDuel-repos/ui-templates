order-amber-900 overflow-hidden\">\n              <div className=\"bg-gradient-to-r from-orange-700 to-red-700 px-6 py-3 border-b-4 border-amber-900\">\n                <h3 className=\"text-lg font-bold text-amber-50\" style={{ fontFamily: 'Courier New, monospace' }}>\n                  ACTIVE COLLABORATORS\n                </h3>\n              </div>\n              <div className=\"p-6 space-y-4\">\n                {collaborators.map((collab) => (\n                  <div key={collab.id} className=\"flex items-center gap-3 bg-amber-50 p-3 rounded-lg border-2 border-amber-900\">\n                    <img \n                      src={collab.avatar} \n                      alt={collab.name}\n                      className=\"w-10 h-10 rounded-full border-2\"\n                      style={{ borderColor: collab.color }}\n                    />\n                    <div className=\"flex-1\">\n                      <div className=\"font-bold text-amber-900\" style={{ fontFamily: 'Courier New, monospace' }}>\n                        {collab.name}\n                      </div>\n                      <div className=\"text-xs text-orange-700\">Editing Qubit {collab.id}</div>\n                    </div>\n                    <div className=\"w-3 h-3 rounded-full animate-pulse\" style={{ backgroundColor: collab.color }}></div>\n                  </div>\n                ))}\n              </div>\n            </div>\n\n            {/* System Status */}\n            <div className=\"bg-amber-100 rounded-lg shadow-xl border-4 border-amber-900 overflow-hidden\">\n              <div className=\"bg-gradient-to-r from-orange-700 to-red-700 px-6 py-3 border-b-4 border-amber-900\">\n                <h3 className=\"text-lg font-bold text-amber-50\" style={{ fontFamily: 'Courier New, monospace' }}>\n                  SYSTEM STATUS\n                </h3>\n              </div>\n              <div className=\"p-6 space-y-3\" style={{ fontFamily: 'Courier New, monospace' }}>\n                <di